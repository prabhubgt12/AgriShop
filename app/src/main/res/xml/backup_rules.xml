<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include SharedPreferences -->
    <include domain="sharedpref" path="." />
    <!-- Include App Databases -->
    <include domain="database" path="." />
    <!-- Include files (app-specific storage), but exclude cache-like dirs if any appear here -->
    <include domain="file" path="." />
    <!-- Note: 'cache' domain is not a valid rule target; caches are excluded by transport -->
</full-backup-content>
